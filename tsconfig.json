{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "target": "ES2024",
    "module": "nodenext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "declaration": true,
  },
  "include": [
    "src/**/*"
  ],
}